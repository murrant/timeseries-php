parameters:
	level: 10
	paths:
		- src
		- tests
	excludePaths:
		- tests/*/data/*
		- vendor/*
	reportUnmatchedIgnoredErrors: true
	treatPhpDocTypesAsCertain: false
	ignoreErrors:
		- '#Call to method PHPUnit\\Framework\\Assert::assert(.*) with (.*) will always evaluate to true.#'

includes:
	- vendor/phpstan/phpstan-phpunit/extension.neon
	- vendor/phpstan/phpstan-symfony/extension.neon
