{
  "name": "librenms/timeseries-php",
  "description": "PHP Timeseries Database Abstraction Library",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Tony Murray",
      "email": "murraytony@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "TimeSeriesPhp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TimeSeriesPhp\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "ext-fileinfo":  "*",
    "ext-simplexml": "*",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^9.5",
    "symfony/var-dumper": "^7.2",
    "laravel/pint": "^1.22",
    "phpstan/phpstan-phpunit": "^2.0",
    "phpstan/phpstan-symfony": "^2.0",
    "symfony/process": "^7.3",
    "rector/rector": "^2.0",
    "symfony/runtime": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/console": "^7.3"
  },
  "require": {
    "php": ">=8.2",
    "ext-sockets": "*",
    "symfony/dependency-injection": "^7.2",
    "symfony/config": "^7.2",
    "symfony/yaml": "^7.2",
    "psr/simple-cache": "^3.0",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^1.0",
    "psr/log": "^3.0",
    "php-http/discovery": "^1.19",
    "nyholm/psr7": "^1.8",
    "php-http/guzzle7-adapter": "^1.0"
  },
  "scripts": {
    "docker:up": "docker compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker compose -f docker/docker-compose.yml down",
    "style": "pint",
    "rector": "rector",
    "test:analyze": "phpstan analyse",
    "test:style": "pint --test",
    "test:rector": "rector --dry-run",
    "test:unit": "phpunit",
    "test:integration": "phpunit --group=integration",
    "integrate":
    [
      "@docker:up",
      "@test:integration",
      "@docker:down"
    ],
    "test": [
      "@test:style",
      "@test:analyze",
      "@test:rector",
      "@test:unit"
    ]
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/runtime": true
    }
  }
}
