{
  "name": "timeseries-php/influxdb2-driver",
  "type": "timeseries-php-driver",
  "require": {
    "php": "^8.3",
    "psr/http-client": "^1.0",
    "psr-discovery/http-client-implementations": "^1.0",
    "psr-discovery/http-factory-implementations": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "\\TimeseriesPhp\\Driver\\InfluxDB2\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "timeseries-php": {
      "driver": "influxdb2",
      "factory": "\\TimeseriesPhp\\Driver\\InfluxDB2\\InfluxFactory",
      "driver-class": "\\TimeseriesPhp\\Driver\\InfluxDB2\\InfluxDriver"
    }
  }
}
