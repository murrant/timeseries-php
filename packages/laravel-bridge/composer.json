{
  "name": "timeseries-php/laravel-bridge",
  "require": {
    "php": "^8.3",
    "illuminate/contracts": "^11.0||^12.0",
    "illuminate/support": "^11.0||^12.0"
  },
  "autoload": {
    "psr-4": {
      "\\TimeseriesPhp\\Bridge\\Laravel\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TimeseriesPhp\\Bridge\\Laravel\\ServiceProvider"
      ],
      "aliases": {
        "TSDB": "TimeseriesPhp\\Bridge\\Laravel\\Facade"
      }
    }
  }
}
