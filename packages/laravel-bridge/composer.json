{
  "name": "timeseries-php/laravel",
  "require": {
    "php": "^8.3",
    "illuminate/support": "^11.0||^12.0",
    "timeseries-php/core": "*",
    "timeseries-php/null-driver": "*"
  },
  "autoload": {
    "psr-4": {
      "\\TimeseriesPhp\\Bridge\\Laravel\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "options": {
        "symlink": true
      },
      "type": "path",
      "url": "../*"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "TimeseriesPhp\\Bridge\\Laravel\\TsdbServiceProvider"
      ],
      "aliases": {
        "TSDB": "TimeseriesPhp\\Bridge\\Laravel\\Facade"
      }
    }
  }
}
